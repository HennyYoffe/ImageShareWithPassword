
@{
    ViewBag.Title = "Upload";
}

<div class="row">
    <a href="/home/myaccount">My Account</a>
    <div class="col-md-6 col-md-offset-3 well">
        <form method="post" action="/home/submitupload" enctype="multipart/form-data">
            <input type="file" name="imageFile" />
            <br />
            <input type="text" class="form-control" name="password" placeholder="Custom Password" />
            <br />
           
            <button class="btn btn-primary">Upload</button>
        </form>
    </div>
</div>